<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Cartel2022" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/kaki_500</item>
        <item name="colorPrimaryVariant">@color/kaki_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/kaki_500</item>
        <item name="colorSecondaryVariant">@color/kaki_500</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.Cartel2022.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Cartel2022.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>



    <style name="Theme.Cartel2022.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>